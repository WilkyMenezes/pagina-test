<!DOCTYPE html>
<html lang="pr-BR">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <title>Document</title>
</head>

<body>


    <nav>
        <ul id="menu">
            <li><a href="/">Home</a></li>
            <li class="md"><a href="/cadastro">Cadastro</a></li>
            <li class="md"><a href="/detalhes">Detalhes</a></li>
        </ul>
    </nav>
    <% if(message != "") {  %>
        <section id="message">
            <p><%= message %></p> <span id="close">&times;</span>
        </section>
    <% }  %>


    <header>
        <h1></h1>
    </header>

        
<main>
    <section id="cards">            
        <% pokedex.forEach((pokemon) => { %>

            <div class="card">
                <img src="<%= pokemon.imagem %>" alt="">
                <h2><%= pokemon.nome %></h2>
                <p><%= pokemon.categoia %></p>
                <p><%= pokemon.tipo %></p>
            </div>

        <% }) %>
    </section>



</main>
    
    <script src="/js/script.js"></script>
</body>

</html>